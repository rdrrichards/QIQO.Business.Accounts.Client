{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "es2015",
    "types": [],
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch":true,
    "noImplicitThis": true,
    "strictNullChecks": true
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
